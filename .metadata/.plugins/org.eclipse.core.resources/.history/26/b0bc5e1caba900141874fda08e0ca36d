package com.example.assignment1;

public class Item {
	protected String itemName; //whatever inherits this can use it
	protected String itemName = name;
	protected String itemDate = date;
	protected String itemCurrency = currency;
	protected int itemCost = cost;
	protected String itemDescription = description;
	
	public Item(String itemName) {
		this.itemName = itemName;
	}

	public void changeName(String newName){
		itemName = newName;
	}

	public Object getName() {//why return object and not string?
		return this.itemName; //apparently the "this" is optional
	}
}
