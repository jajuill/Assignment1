package com.example.assignment1;

import java.util.ArrayList;


public class Expenses {
	
	int value = 0;
	String currencyType;
	
	public Expenses(String newCurrency){
	currencyType = newCurrency;
	}
	public String getCurrency(){
		return currencyType;
	}
	public void changeTotalExpense(int valueChange){
		value += valueChange;
	}
	
}
